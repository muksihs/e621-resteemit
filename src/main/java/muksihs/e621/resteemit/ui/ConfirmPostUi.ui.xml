<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	
	</ui:style>
	<m:MaterialModal ui:field="modal" fullscreen="false">
		<m:MaterialModalContent>
			<m:MaterialTitle style="margin-top: -50px" title="CONFIRM POST"></m:MaterialTitle>
			<m:MaterialButton ui:field="btnPost"
				style='margin: 4px; float:right;' text="POST"></m:MaterialButton>
			<m:MaterialButton ui:field="btnCancel"
				style='margin: 4px; float:right;' text="CANCEL"></m:MaterialButton>
			<m:MaterialLabel text="POST TITLE"></m:MaterialLabel>
			<m:MaterialInput helperText="POST TITLE" type="TEXT"
				ui:field="title">
			</m:MaterialInput>
			<m:MaterialPanel ui:field="tags"></m:MaterialPanel>
			<m:MaterialPanel ui:field="preview"></m:MaterialPanel>
		</m:MaterialModalContent>
	</m:MaterialModal>
</ui:UiBinder> 